
{
  "env": {
      "browser": true,
      "es2021": true,
      "jest/globals": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": ["jest"],
  "rules": {
      "indent": ["error", 4],
      "import/extensions": 0,
      "no-console": 0,
      "object-curly-newline": 0
  }
}